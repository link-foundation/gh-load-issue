{
  "threshold": 10,
  "reporters": ["html", "console"],
  "ignore": [
    "**/node_modules/**",
    "**/build/**",
    "**/dist/**",
    "**/.git/**",
    "**/.changeset/**",
    "**/CHANGELOG.md",
    "**/package-lock.json",
    "**/tests/**",
    "**/scripts/**"
